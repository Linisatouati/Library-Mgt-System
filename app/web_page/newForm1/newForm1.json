    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.selectedTitles ){",
        "\treturn {",
        "\t\t//map selectedTitles variable to expected task contract input",
        "\t\tselectedTitlesInput: $data.selectedTitles.map( it => ({",
        "\t\t\tpersistenceId_string: it.persistenceId_string !== undefined ? it.persistenceId_string : null,",
        "\t\t\ttitle: it.title !== undefined ? it.title : null,",
        "\t\t\tauthor: it.author !== undefined ? it.author : null,",
        "\t\t\tisAvailable: it.isAvailable !== undefined ? it.isAvailable : null",
        "\t\t}))",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "context" : {
      "type" : "url",
      "value" : [
        "../API/bpm/userTask/{{taskId}}/context"
      ],
      "exposed" : false
    },
